<h2><%=l(:label_agile_sprint_new)%></h2>

<%= labelled_form_for @agile_sprint, url: project_agile_sprints_path(@project) do |f| %>
  <%= render partial: 'agile_sprints/form', locals: { f: f } %>
  <%= submit_tag l(:button_create) %>
<% end %>

<% content_for :header_tags do %>
  <%= javascript_include_tag :redmine_agile_sprint, :plugin => 'redmine_agile'  %>
<% end %>
